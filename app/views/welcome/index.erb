<!DOCTYPE html>
<html>
<head>
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script>
$(document).ready(function()
{	
  var i = 0;
      
      
     
       setInterval(function(){
          
				i = parseInt(i)+1; 
				url = "/result/"+i;
               			$.get(url, function(data, status)
               			{       
               			    var rowCount = $('#tab tr').length;
					console.log(data);
					console.log(rowCount);
					if(rowCount>20)
					{
						 $("#tab tr:eq(1)").fadeOut(1000,function()
                                                 {
                                                        $("#tab tr:eq(1)").remove();
                                                       $("#tab tr:eq(2)").remove();
                                                 });
					}	
					                  var s=data["sex"];
					                  var ind=data["unique"];
					                  var y=data["year"];
                        		var row = $("<tr>")
        	               		row.append($("<td>" + ind + "</td>"));
                	       		row.append($("<td>" + s + "</td>"));
                        		row.append($("<td>" + y + "</td>"));
                   			
					$("#tab").append(row);
					row.fadeIn();
               		 	});
           
       
       }, 1000);

      
		//	var cc=setInterval(function(){myTime()},500 );
		
						

})
</script>
